<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:maps="clr-namespace:Microsoft.Maui.Controls.Maps;assembly=Microsoft.Maui.Controls.Maps"
             xmlns:mapsCore="clr-namespace:Microsoft.Maui.Maps;assembly=Microsoft.Maui.Maps"
			 xmlns:essentials="clr-namespace:Microsoft.Maui.Devices.Sensors;assembly=Microsoft.Maui.Essentials"
			 x:Class="Microsoft.Maui.Controls.Xaml.UnitTests.Unreported003" >
	<maps:Map x:Name="map">
		<x:Arguments>
            <mapsCore:MapSpan>
				<x:Arguments>
					<!-- These coordinates land near Red Deer at a fairly high altitude (birds eye view). -->
                    <essentials:Location>
						<x:Arguments>
							<!-- Latitude-->
							<x:Double>52.325</x:Double>
							<!-- Longitude-->
							<x:Double>-113.9</x:Double>
						</x:Arguments>
					</essentials:Location>
					<!-- Elevation lat/long-->
					<x:Double>.8</x:Double>
					<x:Double>.8</x:Double>
				</x:Arguments>
			</mapsCore:MapSpan>
		</x:Arguments>
	</maps:Map>
</ContentPage>